<robot name="test_1">
    <link name="chassis">
        <pose>0 0 0.25 0 0 0</pose>
        <inertial>
            <origin xyz="0 0 0.25" rpy="0 0 0"/>
            <mass value="5"/>
            <inertia ixx="0.046875" ixy="0" ixz="0" iyy="0.11354" iyz="0" izz="0.14167"/>
        </inertial>

        <collision name = "chassis_collision">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.5 0.3 0.15"/>
            </geometry>
        </collision>

        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.5 0.3 0.15"/>
            </geometry>
            <material name="blue">
                <color rgba="0 0.5 1 1"/>
            </material> 
        </visual>                
    </link>

    <link name="front_right_wheel">
        <inertial>
            <origin xyz="0 0 0" rpy="0 1.57 1.57"/>
            <mass value="0.2"/>
            <inertia ixx="0.0005417" ixy="0" ixz="0" iyy="0.0005417" iyz="0" izz="0.001"/>
        </inertial>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 1.57 1.57"/>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
        </collision>

        <visual>
            <origin xyz="0 0 0" rpy="0 1.57 1.57"/>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
    </link>

    <link name="front_left_wheel">
        <inertial>
            <origin xyz="0 0 0" rpy="0 1.57 1.57"/>
            <mass value="0.2"/>
            <inertia ixx="0.0005417" ixy="0" ixz="0" iyy="0.0005417" iyz="0" izz="0.001"/>
        </inertial>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 1.57 1.57"/>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
        </collision>

        <visual>
            <origin xyz="0 0 0" rpy="0 1.57 1.57"/>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
    </link>

    <link name="rear_right_wheel">
        <inertial>
            <origin xyz="0 0 0" rpy="0 1.57 1.57"/>
            <mass value="0.2"/>
            <inertia ixx="0.0005417" ixy="0" ixz="0" iyy="0.0005417" iyz="0" izz="0.001"/>
        </inertial>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 1.57 1.57"/>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
        </collision>

        <visual>
            <origin xyz="0 0 0" rpy="0 1.57 1.57"/>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
    </link>
    
    <link name="rear_left_wheel">
        <inertial>
            <origin xyz="0 0 0" rpy="0 1.57 1.57"/>
            <mass value="0.2"/>
            <inertia ixx="0.0005417" ixy="0" ixz="0" iyy="0.0005417" iyz="0" izz="0.001"/>
        </inertial>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 1.57 1.57"/>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
        </collision>

        <visual>
            <origin xyz="0 0 0" rpy="0 1.57 1.57"/>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
    </link>

    <joint name="front_right_joint" type="continuous">
        <origin xyz="0.15 0.175 0" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="front_right_wheel"/>
        <axis xyz="0 1 0"/>
        <limit effort="1000" velocity="1000"/>
    </joint>
    
    <joint name="front_left_joint" type="continuous">
        <origin xyz="0.15 -0.175 0" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="front_left_wheel"/>
        <axis xyz="0 1 0"/>
        <limit effort="1000" velocity="1000"/>
    </joint>
    
    <joint name="rear_right_joint" type="continuous">
        <origin xyz="-0.15 0.175 0" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="rear_right_wheel"/>
        <axis xyz="0 1 0"/>
        <limit effort="1000" velocity="1000"/>
    </joint>
    
    <joint name="rear_left_joint" type="continuous">
        <origin xyz="-0.15 -0.175 0" rpy="0 0 0"/>
        <parent link="chassis"/>
        <child link="rear_left_wheel"/>
        <axis xyz="0 1 0"/>
        <limit effort="1000" velocity="1000"/>
    </joint>
    
    <gazebo reference="wheel">
        <material>Gazebo/Red</material>
    </gazebo>

    <gazebo reference="chassis">
        <material>Gazebo/Blue</material>
    </gazebo>


</robot>